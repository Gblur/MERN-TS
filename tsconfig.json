{
  "compilerOptions": {
    "jsx": "react",
    "module": "CommonJS",
    "noImplicitAny": true,
    "outDir": "client/dist/",
    "moduleResolution": "node",
    "noEmit": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "**/src"
  ],
  "exclude": ["dist", "build"]
}